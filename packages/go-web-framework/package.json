{
  "name": "go-web-framework",
  "version": "0.0.15",
  "description": "browser javascript for go-web-framework",
  "main": "index.js",
  "type": "module",
  "files": [
    "index.{js,d.ts,js.map,.d.ts.map}",
    "README.md",
    "lib"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    },
    "./*.js": {
      "types": "./lib/*.d.ts",
      "import": "./lib/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:ci": "vitest --browser.headless --run"
  },
  "author": "Ben Bradshaw",
  "license": "MIT",
  "devDependencies": {
    "@vitest/browser-playwright": "^4.0.16",
    "typescript": "^5.8.2",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=20"
  }
}
