{
  "name": "go-web-framework",
  "version": "1.0.0",
  "description": "browser javascript for go-web-framework",
  "main": "index.js",
  "type": "module",
  "files": [
    "index.{js,d.ts,js.map,.d.ts.map}",
    "lib"
  ],
  "exports": {
    "./all": {
      "types": "./index.d.ts",
      "default": "./index.js",
      "import": "./index.js"
    },
    "./*.js": {
      "types": "./lib/*.d.ts",
      "default": "./lib/*.js",
      "import": "./lib/*.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ben Bradshaw",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.7.3"
  }
}
